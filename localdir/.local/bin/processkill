#!/usr/bin/env python3

from sys import argv
from os import kill
from psutil import Process, process_iter

if len(argv) == 1: # scriptname
    for proc in process_iter():
        print("{pid}: {name}".format(pid=proc.pid, name=proc.name()))
else:
    expr = "".join(argv[1:])
    pid = int(expr.split(":")[0])
    kill(pid, 9)

